{
  "name": "action-meticulous-run",
  "version": "1.0.0",
  "description": "Run Meticulous tests",
  "license": "Unliscensed",
  "private": true,
  "type": "module",
  "source": "src/index.ts",
  "main": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "dev": "parcel watch",
    "build": "parcel build",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "lint": "eslint src --ext=ts,tsx,js --cache",
    "lint:commit": "eslint --cache $(git diff --relative --name-only --diff-filter=ACMRTUXB master | grep  -E \"(.js$|.ts$|.tsx$)\")",
    "lint:fix": "eslint src --ext=ts,tsx,js --cache --fix",
    "test": "jest"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "@alwaysmeticulous/client": "^2.44.0",
    "@alwaysmeticulous/common": "^2.44.0",
    "@alwaysmeticulous/replay-orchestrator-launcher": "^2.45.0",
    "@alwaysmeticulous/sentry": "^2.40.3",
    "@sentry/node": "^7.48.0",
    "puppeteer": "19.7.5",
    "lodash.debounce": "^4.0.8",
    "loglevel": "^1.8.1",
    "luxon": "^3.3.0"
  },
  "devDependencies": {
    "@alwaysmeticulous/api": "^2.41.0",
    "@alwaysmeticulous/sdk-bundles-api": "^2.45.0",
    "@parcel/packager-ts": "^2.8.3",
    "@parcel/transformer-typescript-types": "^2.8.3",
    "@types/jest": "^27.0.3",
    "@types/lodash.debounce": "^4.0.7",
    "@types/luxon": "^3.2.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.55.0",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "jest": "^27.4.5",
    "parcel": "^2.8.3",
    "prettier": "^2.8.7",
    "rimraf": "^5.0.0",
    "ts-jest": "^27.1.1",
    "typescript": "^4.9.5",
    "yaml": "^2.2.1"
  },
  "engines": {
    "node": ">= 16"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alwaysmeticulous/action-meticulous-run.git"
  },
  "keywords": [],
  "author": {
    "name": "The Meticulous Team",
    "email": "eng@meticulous.ai",
    "url": "https://meticulous.ai"
  },
  "bugs": {
    "url": "https://github.com/alwaysmeticulous/action-meticulous-run/issues"
  },
  "homepage": "https://github.com/alwaysmeticulous/action-meticulous-run",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
