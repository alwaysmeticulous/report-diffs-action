{
  "name": "action-meticulous-run",
  "version": "1.0.0",
  "description": "Run Meticulous tests",
  "license": "Unliscensed",
  "private": true,
  "main": "dist/main.entrypoint.mjs",
  "cloud-compute": "out/cloud-compute.entrypoint.js",
  "targets": {
    "main": {
      "source": "src/main.entrypoint.ts",
      "distDir": "dist",
      "outputFormat": "esmodule"
    },
    "cloud-compute": {
      "source": "src/cloud-compute.entrypoint.ts",
      "includeNodeModules": true,
      "distDir": "out",
      "sourceMap": false,
      "outputFormat": "commonjs"
    }
  },
  "files": [
    "dist",
    "out"
  ],
  "scripts": {
    "clean": "rimraf dist tsconfig.tsbuildinfo .parcel-cache",
    "dev": "parcel watch",
    "build": "parcel build",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "lint": "eslint src --ext=ts,tsx,js --cache",
    "lint:commit": "eslint --cache $(git diff --relative --name-only --diff-filter=ACMRTUXB master | grep  -E \"(.js$|.ts$|.tsx$)\")",
    "lint:fix": "eslint src --ext=ts,tsx,js --cache --fix",
    "test": "jest"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "@alwaysmeticulous/client": "^2.137.0",
    "@alwaysmeticulous/common": "^2.137.0",
    "@alwaysmeticulous/remote-replay-launcher": "^2.138.0",
    "//": "Upgrading `replay-orchestrator-launcher`? Consider bumping the environment version `LOGICAL_ENVIRONMENT_VERSION` in `constants.ts` if the new version includes visible changes.",
    "@alwaysmeticulous/replay-orchestrator-launcher": "^2.137.0",
    "@alwaysmeticulous/sentry": "^2.137.0",
    "@sentry/node": "^8.19.0",
    "lodash.debounce": "^4.0.8",
    "loglevel": "^1.8.1",
    "luxon": "^3.4.3",
    "retry": "^0.13.1"
  },
  "devDependencies": {
    "@alwaysmeticulous/api": "^2.137.0",
    "@alwaysmeticulous/sdk-bundles-api": "^2.141.0",
    "@parcel/packager-ts": "^2.12.0",
    "@parcel/transformer-typescript-types": "^2.12.0",
    "@types/jest": "^27.0.3",
    "@types/lodash.debounce": "^4.0.9",
    "@types/luxon": "^3.3.2",
    "@types/retry": "^0.12.5",
    "@types/node": "^20.11.28",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.56.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^27.4.5",
    "parcel": "^2.12.0",
    "prettier": "^2.8.7",
    "rimraf": "^5.0.7",
    "ts-jest": "^27.1.1",
    "typescript": "^4.9.5",
    "yaml": "^2.4.1"
  },
  "resolutions": {
    "//": "https://nvd.nist.gov/vuln/detail/CVE-2022-46175",
    "json5": "^2.2.2"
  },
  "engines": {
    "node": ">= 20"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alwaysmeticulous/action-meticulous-run.git"
  },
  "keywords": [],
  "author": {
    "name": "The Meticulous Team",
    "email": "eng@meticulous.ai",
    "url": "https://meticulous.ai"
  },
  "bugs": {
    "url": "https://github.com/alwaysmeticulous/action-meticulous-run/issues"
  },
  "homepage": "https://github.com/alwaysmeticulous/action-meticulous-run",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
